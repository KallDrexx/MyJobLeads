@{
    ViewBag.Title = "Invalid University Registration URL";
}

@section SideBar {
    @{Html.RenderAction(MVC.Home.SidebarDisplay(MyJobLeads.Controllers.ActiveSidebarLink.Register));}
}

<div class="grid1 floatLeft"> 
    <div class="pageInfoBox"> 

            <div class="grid3 marginBottom_10 marginAuto floatLeft"> 
                <h3 class="floatLeft">Invalid University Registration URL</h3> 
            </div> 
                
            <div class="grid3 marginBottom_10 marginAuto floatleft">
                <p class="floatLeft infoSpan">
                    The university registration URL used is not valid.  Please consult with your university for the correct URL for registration.
                </p>
            </div>

            <div class="grid3 marginBottom_10 marginAuto floatLeft"> 
            </div> 

            <div class="clear"></div> 
    </div> 
</div> 
