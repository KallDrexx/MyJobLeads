@model MyJobLeads.DomainModel.ViewModels.ContactSearching.ExternalContactAddedResultViewModel

@{
    ViewBag.Title = "Jigsaw Contact Added";
}

<div class="grid1 floatLeft"> 
    <div class="pageInfoBox"> 
        <div class="grid3 marginBottom_10 marginAuto floatLeft">
            <h3>Contact Added To Your Job Search</h3>
        </div>

        <div class="grid3 marginBottom_10 marginAuto floatLeft">
            <p>
                You have successfully added
                @Html.ActionLink(Model.ContactName, MVC.Contact.Details(Model.ContactId), new { @class = "inlineBlue", title = "View Contact Details" })
                to your job search for the
                @Html.ActionLink(Model.CompanyName, MVC.Company.Details(Model.CompanyId), new { @class = "inlineBlue", title = "View Company Details" }).
            </p>
        </div>
            
        <div class="clear"></div>
    </div>
</div>
